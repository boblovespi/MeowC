let foo: i32 -> i32 := x \mt x * x + x + 10 + 20;

let bar: i32 -> i32 := y \mt foo y + foo(y + 1);

let main: 1 := [
    num: i32;
]
{
    num <- 10;
    print "num: ";
    print num;
    num <- foo num;
    print "\nfoo num: ";
    print num;
    num <- bar num;
    print "\nbar foo num: ";
    print num;
    print "\n";
};